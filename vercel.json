{
  "rewrites": [
    {
      "src": "/sitemap.xml",
      "headers": {
        "Content-Type": "application/xml"
      },
      "dest": "/sitemap.xml"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "regions": ["fra1"]
}
